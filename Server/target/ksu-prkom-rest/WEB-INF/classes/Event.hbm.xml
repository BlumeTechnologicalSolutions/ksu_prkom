<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.lk.entity.Event" table="event_on_calendar" catalog="prkom">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="event" type="java.lang.String">
            <column name="Event" not-null="true"/>
        </property>
        <property name="date" type="timestamp">
            <column name="Date" not-null="true"/>
        </property>
        <property name="education_id" type="java.lang.Integer">
            <column name="Education_Id"  not-null="true" />
        </property>
        <property name="form_education_id" type="java.lang.Integer">
            <column name="Form_Education_Id" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>